script.
  function auto_grow(element) {
      element = document.getElementById("solutionCode");
      element.style.height = "150px";
      element.style.height = (element.scrollHeight)+"px";
  }
  $(document).ready(function() {
      $('#difficult-e,#difficult,#languageCode').niceSelect();
      document.getElementById("solutionCode").oninput = auto_grow;

      $(function() {
        $('#timings').daterangepicker({
          timePicker: true,
          startDate: moment().startOf('hour'),
          endDate: moment().startOf('hour').add(32, 'hour'),
          locale: {
            format: 'YYYY/MM/DD hh:mm A'
          } 
        });
      });

      hljs.configure({ languages: ['javascript', 'python','c','c++','c#']});

      $('[data-toggle="popover"]').popover({
        trigger: "hover",
        delay: {show : 0, hide : 0}
      });

  });
  //language select
  var options = [];

  $( '.dropdown-menu.Lang a' ).on( 'click', function( event ) {
    event.preventDefault();
    var $target = $( event.currentTarget ),
        val = $target.attr( 'data-value' ),
        $inp = $target.find( 'input' ),
        idx;

    if ( ( idx = options.indexOf( val ) ) > -1 ) {
        options.splice( idx, 1 );
        setTimeout( function() { $inp.prop( 'checked', false ) }, 0);
    } else {
        options.push( val );
        setTimeout( function() { $inp.prop( 'checked', true ) }, 0);
    }

    $( event.target ).blur();
        
    
    return false;
  });
  //end of language select
 

